map -count -modes=n,v p -builtin <count>fback1
map -count -modes=n,v J -builtin <count><C-Tab>
set passkeys=www.facebook.com:<CR>,google.com/reader/:npjkm,<S-n>,<S-p>,<S-o>,mail.google.com:!#jknpxeo?,gi,zmail.utdallas.edu:jkco=?
